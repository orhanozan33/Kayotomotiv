datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model AutoSale {
  id        String   @id @default(uuid())
  title     String
  price     Float
  brand     String
  model     String
  year      Int
  createdAt DateTime @default(now())
}

model Admin {
  id       String @id @default(uuid())
  email    String @unique
  password String
}
